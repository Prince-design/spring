<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd ">
	<bean id="Cello" class="com.zensar.spring.instruments.Cello"></bean>
	<bean id="Angus Young" class="com.zensar.spring.performers.Instrumentalist">
	<property name="instrument" value="Cello"></property>
	<property name="song" ref="Thunderstruck"></property>
	</bean>
	
</beans>
